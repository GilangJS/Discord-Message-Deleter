{
    "name": "Delete Discord Messages",
    "version": "0.1",
    "description": "Delete your discord message in batch",
    "browser_action": {
        "default_popup": "view.html"
    },
    "content_scripts": [{
        "matches": ["https://discordapp.com/channels/*"],
        "js": ["localStorage.js"]
    }],
    "permissions": ["storage", "tabs"],
    "manifest_version": 2
}